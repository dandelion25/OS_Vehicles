				<depth>Internal</depth>
				
				<hitbox>
					<cells>
						<li>(0, 0)</li>
					</cells>
				</hitbox>
				
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			
			<li>
				<key>AuxRegulator</key>
				<label>Auxiliary Regulator</label>
				<health>80</health>
