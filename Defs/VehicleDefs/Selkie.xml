				<health>120</health>
				<depth>Internal</depth>
				
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
				</reactors>
				
				<hitbox>
					<cells>
						<li>(0, 0)</li>
						<li>(1, 0)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>Display</key>
				<label>Display</label>
